{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "functions": {
    "src/server.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "buildCommand": "npm run vercel-build"
}
