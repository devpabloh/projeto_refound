{
  "version": 2,
  "builds": [
    {
      "src": "api/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/src/server.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
